@using Task_8.Entities;
@using Task_8.Dependencies

@{
    Layout = "~/Pages/Layout/_BasicLayout.cshtml";

    var usersAndAwardsBll = DependencyResolver.Instance.UsersAndAwardsLogic;
    //var row = usersAndAwardsBll.GetRowFull(int.Parse(Request["idRow"]));

    PageData["Title"] = "UnAssign Award";
    if(!User.IsInRole("admin"))
    {
        Response.Redirect("~/");
    }
}

<a href="~/Pages/User/Details?id=@Request["idUser"]" class="btn btn-primary">Back</a>

@RenderPage("~/Partial/_UnAssign.cshtml", new { idRow = usersAndAwardsBll.GetRowFull(@Guid.Parse(Request["idUser"]), @Guid.Parse(Request["idAward"])).Id })
